<div class="modal" id="modal-subject" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" type="button" data-dismiss="modal">
                    <span class="glyphicon glyphicon-remove"></span>
                </button>
                <h4 class="modat-title" style="text-align: center; vertical-align: middle;">
                    Добавить предмет
                </h4>
            </div>
            <div class="modal-body">
                <form action="/" method="post" id="addFormSubject">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label" for="nameSubject"> Название предмета</label>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                <input type="text" name="Название_предмета" class="form-control" id="nameSubject" autocomplete="off" role="textbox" aria-autocomplete="list" aria-haspopup="true">
                            </div>
                        </div>

                        <div class="row" style="margin-top: 5px;">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label" for="NumberAuditory">Номер аудитории</label>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                <input type="text" name="Номер_аудитории" class="form-control" id="NumberAuditory" autocomplete="off" role="textbox" aria-autocomplete="list" aria-haspopup="true">
                            </div>
                        </div>

                        <div class="row" style="margin-top: 5px;">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label" for="TypeOfOccupation"> Тип занятия</label>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                <select name="Тип_занятия" class="form-control" id="TypeOfOccupation">
                                    <option value="Лекция" selected="selected">Лекция</option>
                                    <option value="Семинар">Семинар</option>
                                    <option value="Лабораторные">Лабораторные</option>
                                </select>
                            </div>
                        </div>

                        <div class="row" style="margin-top: 5px;">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label"> Неделя</label>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                <select name="неделя" class="form-control">
                                    <option value="null" selected="selected">Обычная</option>
                                    <option value="true">Верхняя</option>
                                    <option value="false">Нижняя</option>
                                </select>
                            </div>
                        </div>

                        <div class="row" style="margin-top: 5px;">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label">Единичное</label>
                            </div>
                            <div class="col-md-1 col-sm-1 col-xs-1">
                                <input type="checkbox" id="check_period">
                            </div>
                        </div>

                        <div class="row" style="margin-top: 5px;" id="period">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <label class="control-label">Продолжительность</label>
                            </div>
                            <div class="col-md-1 col-sm-1 col-xs-1">
                                <label class="control-label">c</label>
                            </div>
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <input type="date" name="Начало_занятий" class="form-control" id="Start_period" role="textbox">
                            </div>
                            <div class="col-md-1 col-sm-1 col-xs-1">
                                <label class="control-label">до</label>
                            </div>
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <input type="date" name="Конец_занятий" class="form-control" id="Finish_period" role="textbox">
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button id="CancelSubject" class="btn btn-danger" type="button" data-dismiss="modal"> Отмена</button>
                <button id="ClearSubject" class="btn btn-info" type="button" data-dismiss="modal">Очистить</button>
                <button id="AddSubject" class="btn btn-success" type="button" data-dismiss="modal">Принять</button>
            </div>
        </div>
    </div>
</div>

